{
	network:"firebus",
	password:"firebuspassword0",
	services: [
		{
			type:"objectserver",
			name:"rbos",
			config:{
				configservice:"mongo",
				cacheconfigs:true,
				dataservice:"mongo",
				idgeneratorservice:"rbid",
				accessmanagementservice:"rbam"
			}
		},
		{
			type:"uiserver",
			name:"rbui",
			config:{
				configservice:"mongo",
				resourceservicetype:"filestorage",
				resourceservice:"rbfiles",
				objectservice:"rbos",
				accessmanagementservice:"rbam"
			}
		},
		{
			type:"processserver",
			name:"rbpm",
			config:{
				configservice:"mongo",
				accessmanagementservice:"rbam"
			}
		},
		{
			type:"idgenerator",
			name:"rbid",
			config:{
				configservice:"mongo"
			}
		},
		{
			type:"accessmanager",
			name:"rbam",
			config:{
				configservice:"mongo"
			}
		}		
	],
	adapters:[
		{
			"type":"httplistener",
			"config": {
				"port":"8000",
				"services": ["rbos", "rbui"]		
			}
		},
		{
			"type":"filestorage",
			"servicename":"rbfiles",
			"consumername":"rbfiles",
			"config": {
				"path":"c:\tmp\sharefiles"						
			}
		},
		{
			"type":"mongo",
			"servicename":"mongo",
			"consumername":"mongo",
			"config": {
				"connectionstring":"localhost",
				"database":"local"										
			}
		}
	]
}