{
	network:"firebus",
	password:"firebuspassword0",
	services: [
		{
			type:"objectserver",
			name:"rbos",
			config:{
				configservice:"mongo",
				dataservice:"mongo",
				idgeneratorservice:"rbid"
			}
		},
		{
			type:"uiserver",
			name:"rbui",
			config:{
				configservice:"mongo",
				resourceservicetype:"filestorage",
				resourceservice:"rbfiles",
				objectservice:rbos
			}
		},
		{
			type:"idgenerator",
			name:"rbid",
			config:{
				configservice:"mongo"
			}
		}		
	],
	adapters:[
		{
			"type":"httplistener",
			"config": {
				"port":"8000",
				"services": ["rbos", "rbui"]		
			}
		},
		{
			"type":"filestorage",
			"servicename":"rbfiles",
			"consumername":"rbfiles",
			"config": {
				"path":"C:\temp\sharefiles"						
			}
		},
		{
			"type":"mongo",
			"servicename":"mongo",
			"consumername":"mongo",
			"config": {
				"connectionstring":"localhost",
				"database":"local"						
			}
		}	
	]
}