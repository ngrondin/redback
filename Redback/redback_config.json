{
	network:"firebus",
	password:"firebuspassword0",
	services: [
		{
			type:"objectserver",
			name:"rbos",
			config:{
				configservice:"mongo",
				dataservice:"mongo",
				idgeneratorservice:"rbid",
				accessmanagementservice:"rbam",
				globalvariables:{
					"processservicename":"rbpm"
				}
			}
		},
		{
			type:"uiserver",
			name:"rbui",
			config:{
				configservice:"mongo",
				resourceservicetype:"filestorage",
				resourceservice:"rbfiles",
				objectservice:"rbos",
				accessmanagementservice:"rbam"
			}
		},
		{
			type:"processserver",
			name:"rbpm",
			config:{
				configservice:"mongo",
				accessmanagementservice:"rbam",
				sysusername:"processuser",
				sysuserpassword:"poil",
				globalvariables:{
					"objectservicename":"rbos"
				}
			}
		},
		{
			type:"idgenerator",
			name:"rbid",
			config:{
				configservice:"mongo"
			}
		},
		{
			type:"accessmanager",
			name:"rbam",
			config:{
				configservice:"mongo"
			}
		}		
	],
	adapters:[
		{
			"type":"httplistener",
			"config": {
				"port":"8000",
				"services": ["rbos", "rbui", "rbpm"]		
			}
		},
		{
			"type":"filestorage",
			"servicename":"rbfiles",
			"consumername":"rbfiles",
			"config": {
				"path":"D:\Scratch\sharefiles"						
			}
		},
		{
			"type":"mongo",
			"servicename":"mongo",
			"consumername":"mongo",
			"config": {
				"connectionstring":"192.168.1.4",
				"database":"local"						
			}
		}
	]
}